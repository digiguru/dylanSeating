{
    "name": "dylan-seating",
    "version": "0.0.1",
    "type": "module",
    "dependencies": {
        "express": "^4.17.1",
        "jquery": "^3.6.0",
        "mongodb": "^3.6.4",
        "mongojs": "^3.1.0",
        "mongoose": "^5.11.19",
        "raphael": "^2.3.0",
        "socket.io": "^3.1.2",
        "underscore": "^1.13.1"
    },
    "jest-junit": {
        "outputDirectory": "results"
    },
    "jest": {
