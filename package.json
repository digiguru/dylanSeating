        "node": "16.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "babel-jest": "^27.3.1",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "http-server": "^14.0.0",
        "jest": "^27.3.1",
        "jest-environment-node": "^27.3.1",
        "jest-junit": "^13.0.0",
        "jest-puppeteer": "^6.0.0",
